const solution = (s : string) => {
    let answer = true;
    
    if(s.indexOf('e') > 0 ) {
        if(isNaN(s.replaceAll('e','p'))) return false;
    }else{
        if(isNaN(s)) return false;
    }
    
    if(s.length != 4 && s.length != 6) return false;
    
    return answer;
}
export default solution;